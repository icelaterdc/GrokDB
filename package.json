{
  "name": "grokdb",
  "version": "1.3.1",
  "description": "A modern, type-safe SQLite database wrapper",
  "repository": {
    "type": "git",
    "url": "https://github.com/icelaterdc/GrokDB.git"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "grokdb": "./dist/cli.js"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "dev": "tsc --watch",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "cli": "node dist/cli.js"
  },
  "keywords": [
    "database",
    "sqlite",
    "better-sqlite3",
    "typescript",
    "orm",
    "query-builder",
    "migrations",
    "cli",
    "grok",
    "grokdb",
    "grok database",
    "GrokDB"
  ],
  "author": "IceLater",
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^9.4.3",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "crypto-js": "^4.2.0",
    "inquirer": "^9.2.15",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.9",
    "@types/crypto-js": "^4.2.1",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  }
}
